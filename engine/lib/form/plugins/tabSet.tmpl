<div class="{{entry.class}}">
	<label ng-bind-html="entry.label|toTrustedHtml" class=""></label>
	<tabset>
		<tab ng-repeat="oneTab in entry.tabs" heading="{{oneTab.label}}">
			<table border="0" cellspacing="0" cellpadding="0" width="100%">
				<tr ng-repeat="entry in oneTab.entries" class="form-group tr-{{entry.name}}-wrapper" ng-switch="entry.type">
					<td ng-if="entry.directive">
						<div id="{{entry.name}}-wrapper" ng-include="entry.directive"></div>
					</td>
					<td ng-switch-when="textarea">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/textarea.tmpl'"></div>
					</td>

					<td ng-switch-when="editor">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/editor.tmpl'"></div>
					</td>

					<td ng-switch-when="html" class="typeHtml">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/html.tmpl'"></div>
					</td>
					
					<td ng-switch-when="accordion">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/accordion.tmpl'"></div>
					</td>
					
					<td ng-switch-when="group">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/group.tmpl'"></div>
					</td>
					
					<td ng-switch-when="uiselect" class="form-select">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/uiselect.tmpl'"></div>
					</td>
					
					<td ng-switch-when="radio">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/radio.tmpl'"></div>
					</td>

					<td ng-switch-when="select" class="form-select">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/select.tmpl'"></div>
					</td>

					<td ng-switch-when="checkbox" class="form-checkbox">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/checkbox.tmpl'"></div>
					</td>

					<td ng-switch-when="multi-select">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/multi-select.tmpl'"></div>
					</td>

					<td ng-switch-when="readonly">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/readonly.tmpl'"></div>
					</td>

					<td ng-switch-when="date-picker">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/date-picker.tmpl'"></div>
					</td>

					<td ng-switch-when="document">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/document.tmpl'"></div>
					</td>

					<td ng-switch-when="audio">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/audio.tmpl'"></div>
					</td>

					<td ng-switch-when="video">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/video.tmpl'"></div>
					</td>

					<td ng-switch-when="image">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/image.tmpl'"></div>
					</td>

					<td ng-switch-when="number">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/number.tmpl'"></div>
					</td>

					<td ng-switch-when="phone">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/phone.tmpl'"></div>
					</td>

					<td ng-switch-when="json">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/json.tmpl'"></div>
					</td>

					<td ng-switch-when="jsoneditor">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/jsoneditor.tmpl'"></div>
					</td>
					
					<td ng-switch-when="ckeditor">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/ckeditor.tmpl'"></div>
					</td>
					
					<td ng-switch-when="buttonSlider">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/buttonSlider.tmpl'"></div>
					</td>

					<td ng-if="!entry.directive" ng-switch-default="">
						<div id="{{entry.name}}-wrapper" ng-include="'engine/lib/form/plugins/default.tmpl'"></div>
					</td>
				</tr>
			</table>
		</tab>
	</tabset>

</div>
